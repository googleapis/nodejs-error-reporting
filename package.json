{
  "name": "@google-cloud/error-reporting",
  "description": "Stackdriver Error Reporting Client Library for Node.js",
  "version": "0.2.2",
  "license": "Apache-2.0",
  "author": "Google Inc.",
  "engines": {
    "node": ">=4.0"
  },
  "repository": "googleapis/nodejs-error-reporting",
  "main": "./src/index.js",
  "nyc": {
    "exclude": [
      "./utils/fuzzer.js"
    ]
  },
  "files": [
    "src",
    "utils",
    "index.js"
  ],
  "contributors": [
    "Ace Nassri <anassri@google.com>",
    "Ali Ijaz Sheikh <ofrobots@google.com>",
    "Cristian Cavalli <cristiancavall@google.com>",
    "Dominic Kramer <DominicDKramer@google.com>",
    "Eric Uldall <ericuldall@gmail.com>",
    "Jason Dobry <jdobry@google.com>",
    "Kelvin Jin <kelvinjin@google.com>",
    "Luke Sneeringer <lukesneeringer@google.com>",
    "Stephen Sawchuk <sawchuk@gmail.com>",
    "Tomoaki Abe <abe@enzou.tokyo>"
  ],
  "scripts": {
    "docs": "repo-tools exec -- jsdoc -c .jsdoc.js",
    "generate-scaffolding": "repo-tools generate all && repo-tools generate lib_samples_readme -l samples/ --config ../.cloud-repo-tools.json",
    "lint": "repo-tools lint --cmd eslint -- src/ samples/ system-test/ test/",
    "prettier": "repo-tools exec -- prettier --write src/*.js src/*/*.js samples/*.js samples/*/*.js test/*.js test/*/*.js test/*/*/*.js system-test/*.js system-test/*/*.js",
    "system-test": "nyc --exclude=\"error-message.js\" mocha ./system-test/*.js",
    "cover": "nyc --exclude=\"fuzzer.js\" --reporter=lcov mocha --require intelli-espower-loader ./test/unit/*.js ./test/unit/**/*.js && nyc report",
    "samples-test": "cd samples/ && npm link ../ && npm test && cd ../",
    "test-no-cover": "repo-tools test run --cmd mocha -- ./test/unit/*.js ./test/unit/**/*.js --no-timeouts",
    "test": "repo-tools test run --cmd npm -- run cover"
  },
  "dependencies": {
    "@google-cloud/common": "^0.15.1",
    "extend": "^3.0.0",
    "is": "^3.2.0",
    "lodash.has": "^4.5.2"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "^2.0.11",
    "async": "^2.6.0",
    "body-parser": "^1.15.1",
    "codecov": "^3.0.0",
    "eslint": "^4.9.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.3.1",
    "express": "^4.13.4",
    "hapi": "^17.1.1",
    "ink-docstrap": "^1.3.0",
    "intelli-espower-loader": "^1.0.1",
    "jscs": "^3.0.7",
    "jsdoc": "^3.5.5",
    "jshint": "^2.9.2",
    "koa": "^2.4.1",
    "lodash.assign": "^4.2.0",
    "lodash.foreach": "^4.5.0",
    "lodash.indexof": "^4.0.5",
    "lodash.maxby": "^4.6.0",
    "lodash.merge": "^4.6.0",
    "lodash.omit": "^4.5.0",
    "lodash.omitby": "^4.6.0",
    "lodash.pick": "^4.4.0",
    "lodash.random": "^3.2.0",
    "lodash.without": "^4.4.0",
    "mocha": "^4.0.1",
    "nock": "^9.0.0",
    "nyc": "^11.3.0",
    "power-assert": "^1.4.4",
    "prettier": "^1.7.4",
    "proxyquire": "^1.7.11",
    "restify": "^6.3.4"
  }
}
