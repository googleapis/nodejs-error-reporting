    "codecov": "^3.5.0",
    "delay": "^4.3.0",
    "express": "^4.17.1",
    "gts": "^3.0.0-alpha.1",
    "hapi": "^18.1.0",
    "js-green-licenses": "^2.0.0",
    "json-stable-stringify": "^1.0.1",
    "koa": "^2.7.0",
    "linkinator": "^2.0.0",
    "lodash.maxby": "^4.6.0",
    "lodash.merge": "^4.6.2",
    "lodash.omit": "^4.5.0",
    "lodash.omitby": "^4.6.0",
    "lodash.pick": "^4.4.0",
